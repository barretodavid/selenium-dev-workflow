{
  "name": "tip-build",
  "version": "1.0.0",
  "main": "index.js",
  "author": "David Barreto <barretollano@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./s3-bucket/watch-folder.js",
    "serve": "node ./server/app.js",
    "clear:loader": "rm -rf dist/loader",
    "clear:server": "rm -rf dist/server",
    "clear:app": "rm -rf dist/app",
    "clear": "rm -rf dist",
    "build:loader:dev": "yarn clear:loader && NODE_ENV=development webpack --config webpack.config.loader.js",
    "build:loader:prod": "yarn clear:loader && NODE_ENV=production webpack --config webpack.config.loader.js",
    "build:loader": "yarn build:loader:dev",
    "build:server": "yarn clear:server && tsc --project tsconfig.server.json",
    "build:app:dev": "yarn clear:app && NODE_ENV=development webpack --config webpack.config.app.js",
    "build:app:prod": "yarn clear:app && NODE_ENV=production webpack --config webpack.config.app.js",
    "build:app": "yarn build:app:dev",
    "build:dev": "yarn build:loader:dev | yarn build:server | yarn build:app:dev",
    "build:prod": "yarn build:loader:prod | yarn build:server | yarn build:app:prod",
    "build": "yarn build:loader | yarn build:server | yarn build:app",
    "deploy:loader": "ssh into aws and send file",
    "deploy:server": "",
    "deploy:app": "",
    "deploy:all": "",
    "deploy": "yarn deploy:all",
    "start:server": "node dist/server/index.js",
    "start:loader": "node ./scripts/dev-server.loader.js",
    "start:app": "node ./scripts/dev-server.app.js"
  },
  "dependencies": {
    "chokidar": "^1.6.1",
    "socket.io": "^1.7.4",
    "socket.io-client": "^1.7.4"
  },
  "devDependencies": {
    "@types/chokidar": "^1.6.0",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.18",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.29",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
