{
  "name": "tip-build",
  "version": "1.0.0",
  "main": "index.js",
  "author": "David Barreto <barretollano@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch ./dist ./scripts/start-dev-server.js",
    "open": "node ./scripts/open-dev-browser.js",
    "selenium": "java -jar -Dwebdriver.chrome.driver=./selenium/chromedriver ./selenium/selenium-server-standalone-3.0.1.jar",
    "clear": "rm -rf dist",
    "build:dev": "yarn clear && NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "yarn clear && NODE_ENV=production webpack --config webpack.config.js",
    "build:watch": "yarn clear && NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "yarn build:app:dev",
    "server": "watch-http-server ./dist"
  },
  "dependencies": {
    "chokidar": "^1.6.1",
    "socket.io": "^1.7.4",
    "socket.io-client": "^1.7.4"
  },
  "devDependencies": {
    "@types/chokidar": "^1.6.0",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.18",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.29",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "watch-http-server": "^0.7.6",
    "webdriverio": "^4.8.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
